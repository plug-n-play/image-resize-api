{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "description": "Starter project for creating Express APIs with TypeScript, Jasmine and ESLint",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jasmine": "jasmine",
    "build": "npx tsc",
    "test": "npm run build && npm run jasmine"
  },
  "keywords": [],
  "author": "Amandeep Singh",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0"
  }
}
